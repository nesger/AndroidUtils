<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <com.zengyu.wheel.widgets.DebounceEditText
        android:id="@+id/edt_xml"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        app:debounce_time="1000"
        android:hint="xml"
        />
    <TextView
        android:id="@+id/tv_xml"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <com.zengyu.wheel.widgets.DebounceEditText
        android:id="@+id/edt_default"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:hint="default"
        />
    <TextView
        android:id="@+id/tv_default"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <com.zengyu.wheel.widgets.DebounceEditText
        android:id="@+id/edt_program"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:hint="program"
        />
    <TextView
        android:id="@+id/tv_program"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
</LinearLayout>